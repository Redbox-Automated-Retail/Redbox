    <Style
        x:Key="border_button_style"
        TargetType="{x:Type Redbox:BorderButton}">
        <Setter
            Property="BorderThickness"
            Value="3" />
        <Setter
            Property="Background"
            Value="#FFFFFFFF" />
        <Setter
            Property="Foreground"
            Value="{StaticResource RubineRedBrush}" />
        <Setter
            Property="BorderBrush"
            Value="{StaticResource RubineRedBrush}" />
        <Setter
            Property="Redbox:BorderButton.AutoCornerRadius"
            Value="True" />
        <Setter
            Property="Redbox:BorderButton.IsSelected"
            Value="True" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type Redbox:BorderButton}">
                    <Border
                        BorderThickness="0"
                        Background="{Binding Path=Background, RelativeSource={RelativeSource TemplatedParent}}">
                        <Grid>
                            <Border
                                Background="#00FFFFFF"
                                BorderBrush="{Binding Path=BorderBrush, RelativeSource={RelativeSource TemplatedParent}}"
                                CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}}"
                                BorderThickness="{Binding Path=BorderThickness, RelativeSource={RelativeSource TemplatedParent}}" />
                            <Border
                                Background="#00FFFFFF"
                                BorderThickness="3"
                                Margin="-2"
                                BorderBrush="{Binding Path=BorderBrush, RelativeSource={RelativeSource TemplatedParent}}"
                                CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                                <UIElement.Effect>
                                    <DropShadowEffect
                                        Direction="310"
                                        ShadowDepth="3"
                                        Opacity="1"
                                        RenderingBias="Quality"
                                        BlurRadius="7"
                                        Color="{Binding Path=EffectColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                </UIElement.Effect>
                            </Border>
                            <Border
                                Margin="30,18,30,10"
                                Background="#00FFFFFF">
                                <UIElement.Effect>
                                    <DropShadowEffect
                                        Direction="0"
                                        Color="Transparent"
                                        ShadowDepth="0"
                                        Opacity="1"
                                        RenderingBias="Quality"
                                        BlurRadius="0" />
                                </UIElement.Effect>
                                <ContentPresenter
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center" />
                            </Border>
                        </Grid>
                        <UIElement.Clip>
                            <RectangleGeometry
                                Rect="{Binding Path=BorderRect, RelativeSource={RelativeSource TemplatedParent}}"
                                RadiusX="{Binding Path=SingleRadius, RelativeSource={RelativeSource TemplatedParent}}"
                                RadiusY="{Binding Path=SingleRadius, RelativeSource={RelativeSource TemplatedParent}}" />
                        </UIElement.Clip>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition
                        Property="Redbox:BorderButton.IsSelected"
                        Value="True" />
                    <Condition
                        Property="Redbox:BorderButton.ButtonState"
                        Value="0" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter
                        Property="Redbox:BorderButton.EffectColor"
                        Value="{StaticResource ActiveBorder}" />
                    <Setter
                        Property="BorderBrush"
                        Value="{StaticResource ActiveBorderBrush}" />
                </MultiTrigger.Setters>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition
                        Property="Redbox:BorderButton.IsSelected"
                        Value="False" />
                    <Condition
                        Property="Redbox:BorderButton.ButtonState"
                        Value="0" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter
                        Property="Redbox:BorderButton.EffectColor"
                        Value="{StaticResource InactiveBorder}" />
                    <Setter
                        Property="BorderBrush"
                        Value="{StaticResource InactiveBorderBrush}" />
                </MultiTrigger.Setters>
            </MultiTrigger>
            <Trigger
                Property="Redbox:BorderButton.ButtonState"
                Value="1">
                <Setter
                    Property="Redbox:BorderButton.EffectColor"
                    Value="{StaticResource ErrorBorder}" />
                <Setter
                    Property="BorderBrush"
                    Value="{StaticResource ErrorBorderBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="group_button_style"
        TargetType="{x:Type Redbox:GroupButton}">
        <Setter
            Property="Foreground"
            Value="#FFFFFFFF" />
        <Setter
            Property="BorderThickness"
            Value="0" />
        <Setter
            Property="BorderBrush"
            Value="{StaticResource WarmPurpleBrush}" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type Redbox:GroupButton}">
                    <Border
                        Name="border"
                        BorderThickness="3"
                        Background="{TemplateBinding Control.Background}"
                        BorderBrush="{StaticResource WarmPurpleBrush}">
                        <ContentPresenter
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger
                            Property="UIElement.IsMouseOver"
                            Value="True" />
                        <Trigger
                            Property="Redbox:GroupButton.IsSelected"
                            Value="True">
                            <Setter
                                Property="Background"
                                Value="{StaticResource WarmPurpleBrush}" />
                            <Setter
                                Property="Redbox:RoundedButton.CornerRadius"
                                Value="6" />
                        </Trigger>
                        <Trigger
                            Property="Redbox:GroupButton.IsSelected"
                            Value="False">
                            <Setter
                                Property="Foreground"
                                Value="{StaticResource WarmPurpleBrush}" />
                            <Setter
                                Property="Background"
                                Value="#FFFFFFFF" />
                            <Setter
                                Property="BorderThickness"
                                Value="3" />
                            <Setter
                                Property="BorderBrush"
                                Value="{StaticResource WarmPurpleBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="sectional_bar_style"
        TargetType="{x:Type Redbox:SectionalBar}">
        <Setter
            Property="Margin"
            Value="0,0,0,0" />
        <Style.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition
                        Property="Redbox:SectionalBar.IsFront"
                        Value="False" />
                    <Condition
                        Property="Redbox:SectionalBar.IsBack"
                        Value="False" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter
                        Property="Template">
                        <Setter.Value>
                            <ControlTemplate
                                TargetType="{x:Type Redbox:SectionalBar}">
                                <Redbox:GridExt
                                    MinHeight="5"
                                    MinWidth="50">
                                    <Border
                                        Name="PART_Track"
                                        CornerRadius="0"
                                        BorderThickness="0"
                                        Background="#00FFFFFF" />
                                    <Border
                                        Name="PART_Indicator"
                                        CornerRadius="0"
                                        BorderThickness="0"
                                        HorizontalAlignment="Left"
                                        Background="{StaticResource RubineRedGradientBrush}" />
                                </Redbox:GridExt>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </MultiTrigger.Setters>
            </MultiTrigger>
            <Trigger
                Property="Redbox:SectionalBar.IsFront"
                Value="True">
                <Setter
                    Property="Template">
                    <Setter.Value>
                        <ControlTemplate
                            TargetType="{x:Type Redbox:SectionalBar}">
                            <Grid
                                MinHeight="5"
                                MinWidth="50">
                                <Border
                                    Name="PART_Track"
                                    CornerRadius="{TemplateBinding Redbox:SectionalBar.FrontCornerRadius}"
                                    BorderThickness="0"
                                    Background="#00FFFFFF" />
                                <Border
                                    Name="PART_Indicator"
                                    CornerRadius="{TemplateBinding Redbox:SectionalBar.FrontCornerRadius}"
                                    BorderThickness="0"
                                    HorizontalAlignment="Left"
                                    Background="{StaticResource RubineRedGradientBrush}" />
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Trigger>
            <Trigger
                Property="Redbox:SectionalBar.IsBack"
                Value="True">
                <Setter
                    Property="Template">
                    <Setter.Value>
                        <ControlTemplate
                            TargetType="{x:Type Redbox:SectionalBar}">
                            <Grid
                                MinHeight="5"
                                MinWidth="50">
                                <Border
                                    Name="PART_Track"
                                    CornerRadius="{TemplateBinding Redbox:SectionalBar.BackCornerRadius}"
                                    BorderThickness="0"
                                    Background="#00FFFFFF" />
                                <Border
                                    Name="PART_Indicator"
                                    CornerRadius="{TemplateBinding Redbox:SectionalBar.BackCornerRadius}"
                                    BorderThickness="0"
                                    HorizontalAlignment="Left"
                                    Background="{StaticResource RubineRedGradientBrush}" />
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
        x:Key="flat_rounded_button_style"
        TargetType="{x:Type Redbox:RoundedButton}">
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type Redbox:RoundedButton}">
                    <Grid>
                        <Border
                            Name="InnerBorder"
                            CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                            <Border.Background>
                                <LinearGradientBrush
                                    EndPoint="{Binding Path=BorderGradientEndPoint, RelativeSource={RelativeSource TemplatedParent}}"
                                    StartPoint="{Binding Path=BorderGradientStartPoint, RelativeSource={RelativeSource TemplatedParent}}">
                                    <GradientStop
                                        Offset="0"
                                        Color="{Binding Path=BorderGradientColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                    <GradientStop
                                        Offset="1"
                                        Color="{Binding Path=BorderColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <Border
                                Name="InnerSubBorder"
                                CornerRadius="{Binding Path=CornerRadiusInner, RelativeSource={RelativeSource TemplatedParent}}"
                                Margin="{Binding Path=BorderThickness, RelativeSource={RelativeSource TemplatedParent}}">
                                <Border.Background>
                                    <SolidColorBrush
                                        Color="White" />
                                </Border.Background>
                            </Border>
                        </Border>
                        <Border
                            Name="GlassHighlight">
                            <Border.Background>
                                <SolidColorBrush
                                    Color="Transparent" />
                            </Border.Background>
                        </Border>
                        <ContentPresenter
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="glass_rounded_button_style"
        TargetType="{x:Type Redbox:RoundedButton}">
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type Redbox:RoundedButton}">
                    <Grid>
                        <Border
                            Background="#FFD3D3D3"
                            Margin="-1"
                            CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}}"
                            Opacity="{Binding Path=DropShadowOpacity, RelativeSource={RelativeSource TemplatedParent}}">
                            <UIElement.Effect>
                                <BlurEffect
                                    Radius="3" />
                            </UIElement.Effect>
                        </Border>
                        <Border
                            Name="InnerBorder"
                            CornerRadius="{Binding Path=CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                            <Border.Background>
                                <LinearGradientBrush
                                    EndPoint="{Binding Path=BorderGradientEndPoint, RelativeSource={RelativeSource TemplatedParent}}"
                                    StartPoint="{Binding Path=BorderGradientStartPoint, RelativeSource={RelativeSource TemplatedParent}}">
                                    <GradientStop
                                        Offset="0"
                                        Color="{Binding Path=BorderGradientColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                    <GradientStop
                                        Offset="1"
                                        Color="{Binding Path=BorderColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <UIElement.Effect>
                                <DropShadowEffect
                                    Color="#606060"
                                    Direction="-50"
                                    ShadowDepth="{Binding Path=DropShadowDepth, RelativeSource={RelativeSource TemplatedParent}}"
                                    Opacity="{Binding Path=DropShadowOpacity, RelativeSource={RelativeSource TemplatedParent}}" />
                            </UIElement.Effect>
                            <Border
                                Name="InnerSubBorder"
                                CornerRadius="{Binding Path=CornerRadiusInner, RelativeSource={RelativeSource TemplatedParent}}"
                                Margin="{Binding Path=BorderThickness, RelativeSource={RelativeSource TemplatedParent}}">
                                <Border.Background>
                                    <LinearGradientBrush
                                        EndPoint="{Binding Path=BackgroundGradientEndPoint, RelativeSource={RelativeSource TemplatedParent}}"
                                        StartPoint="{Binding Path=BackgroundGradientStartPoint, RelativeSource={RelativeSource TemplatedParent}}">
                                        <GradientStop
                                            Offset="0"
                                            Color="{Binding Path=BackgroundGradientColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                        <GradientStop
                                            Offset="1"
                                            Color="{Binding Path=BackgroundColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Border>
                        <Border
                            Name="GlassHighlight"
                            Opacity=".5"
                            Margin="0,5,0,20"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            CornerRadius="{Binding Path=GlassHighlightCornerRadius, RelativeSource={RelativeSource TemplatedParent}}"
                            Height="{Binding Path=GlassHighlightHeight, RelativeSource={RelativeSource TemplatedParent}}"
                            Width="{Binding Path=GlassHighlightWidth, RelativeSource={RelativeSource TemplatedParent}}"
                            Visibility="{Binding Path=GlassHighlightVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                            <Border.Background>
                                <LinearGradientBrush
                                    EndPoint="0.5,1"
                                    StartPoint="0.5,0">
                                    <GradientStop
                                        Color="White"
                                        Offset="0" />
                                    <GradientStop
                                        Offset="1"
                                        Color="{Binding Path=BackgroundGradientColor, RelativeSource={RelativeSource TemplatedParent}}" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <ContentPresenter
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="base_disabled_glass_rounded_button_style"
        TargetType="{x:Type Redbox:RoundedButton}"
        BasedOn="{StaticResource glass_rounded_button_style}">
        <Setter
            Property="Redbox:RoundedButton.CornerRadius"
            Value="17" />
        <Setter
            Property="BorderThickness"
            Value="3" />
        <Style.Triggers>
            <Trigger
                Property="IsEnabled"
                Value="False">
                <Setter
                    Property="Redbox:RoundedButton.BorderColor"
                    Value="#1D1D1D" />
                <Setter
                    Property="Redbox:RoundedButton.BorderGradientColor"
                    Value="#1D1D1D" />
                <Setter
                    Property="Redbox:RoundedButton.BackgroundColor"
                    Value="#1D1D1D" />
                <Setter
                    Property="Redbox:RoundedButton.BackgroundGradientColor"
                    Value="#363636" />
                <Setter
                    Property="Foreground"
                    Value="#636363" />
            </Trigger>
        </Style.Triggers>
    </Style>
