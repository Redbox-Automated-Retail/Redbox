    <Style
        x:Key="two_discs_icon"
        TargetType="{x:Type UserControl}">
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type UserControl}">
                    <Grid>
                        <Path
                            Stroke="#FF000000"
                            StrokeThickness="5">
                            <Path.Data>
                                <PathGeometry>
                                    <PathFigure
                                        StartPoint="76,90">
                                        <ArcSegment
                                            Size="47.5,47.5"
                                            SweepDirection="Clockwise"
                                            IsLargeArc="True"
                                            Point="76,10" />
                                    </PathFigure>
                                </PathGeometry>
                            </Path.Data>
                        </Path>
                        <Path
                            Stroke="#FF000000"
                            StrokeThickness="5">
                            <Path.Data>
                                <PathGeometry>
                                    <PathFigure
                                        StartPoint="55,63">
                                        <ArcSegment
                                            Size="15,15"
                                            SweepDirection="Clockwise"
                                            IsLargeArc="True"
                                            Point="55,36" />
                                    </PathFigure>
                                </PathGeometry>
                            </Path.Data>
                        </Path>
                        <Ellipse
                            VerticalAlignment="Top"
                            HorizontalAlignment="Left"
                            Margin="50,0,0,0"
                            Width="100"
                            Height="100"
                            StrokeThickness="5"
                            Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
                        <Ellipse
                            HorizontalAlignment="Left"
                            Margin="82.5,0,0,0"
                            Width="35"
                            Height="35"
                            StrokeThickness="5"
                            VerticalAlignment="Center"
                            Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="handicap_icon"
        TargetType="{x:Type UserControl}">
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type UserControl}">
                    <Grid>
                        <Ellipse
                            VerticalAlignment="Top"
                            HorizontalAlignment="Left"
                            Margin="18,17,0,0"
                            Width="27"
                            Height="27"
                            StrokeThickness="4"
                            Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}">
                            <UIElement.Clip>
                                <GeometryGroup
                                    FillRule="Nonzero">
                                    <RectangleGeometry
                                        Rect="0,0,7,30" />
                                    <RectangleGeometry
                                        Rect="7,8,20,30">
                                        <Geometry.Transform>
                                            <RotateTransform
                                                CenterX="13"
                                                CenterY="17"
                                                Angle="70" />
                                        </Geometry.Transform>
                                    </RectangleGeometry>
                                </GeometryGroup>
                            </UIElement.Clip>
                        </Ellipse>
                        <Ellipse
                            VerticalAlignment="Top"
                            HorizontalAlignment="Left"
                            Margin="25,4,0,0"
                            Width="8"
                            Height="8"
                            Fill="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
                        <Line
                            X1="28"
                            Y1="20"
                            X2="40"
                            Y2="20"
                            StrokeThickness="4"
                            Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
                        <Path
                            x:Name="Finger1"
                            StrokeThickness="4"
                            Width="150"
                            Height="250"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Margin="0,0"
                            Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}">
                            <Path.Data>
                                <PathGeometry>
                                    <PathGeometry.Figures>
                                        <PathFigure
                                            StartPoint="28,8">
                                            <PathFigure.Segments>
                                                <PathSegmentCollection>
                                                    <LineSegment
                                                        Point="29,26" />
                                                    <LineSegment
                                                        Point="42,26" />
                                                    <LineSegment
                                                        Point="46,38" />
                                                    <LineSegment
                                                        Point="52,36" />
                                                </PathSegmentCollection>
                                            </PathFigure.Segments>
                                        </PathFigure>
                                    </PathGeometry.Figures>
                                </PathGeometry>
                            </Path.Data>
                        </Path>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
