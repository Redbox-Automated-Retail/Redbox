        x:Key="red_glass_rounded_button_style"
        TargetType="{x:Type Redbox:RoundedButton}"
        BasedOn="{StaticResource base_disabled_glass_rounded_button_style}">
        <Setter
            Property="Redbox:RoundedButton.BorderColor"
            Value="#8F0E15" />
        <Setter
            Property="Redbox:RoundedButton.BorderGradientColor"
            Value="#8F0E15" />
        <Setter
            Property="Redbox:RoundedButton.BackgroundColor"
            Value="#B50E1B" />
