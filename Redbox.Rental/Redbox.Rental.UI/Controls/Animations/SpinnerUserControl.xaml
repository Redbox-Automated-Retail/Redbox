<UserControl
    x:Class="Redbox.Rental.UI.Controls.Animations.SpinnerUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid
        x:Name="Spinner"
        RenderTransformOrigin="0.5,0.5">
        <Rectangle
            Fill="#999999"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="30" />
                    <TranslateTransform
                        X="6"
                        Y="-10" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="60" />
                    <TranslateTransform
                        X="10"
                        Y="-6" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="90" />
                    <TranslateTransform
                        X="12" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="-60" />
                    <TranslateTransform
                        X="10"
                        Y="6" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="-30" />
                    <TranslateTransform
                        X="6"
                        Y="10" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform />
                    <TranslateTransform
                        Y="12" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="30" />
                    <TranslateTransform
                        X="-6"
                        Y="10" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#A3A3A3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="60" />
                    <TranslateTransform
                        X="-10"
                        Y="6" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#BCBCBC"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="90" />
                    <TranslateTransform
                        X="-12" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#E3E3E3"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="-60" />
                    <TranslateTransform
                        X="-10"
                        Y="-6" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#EFEFEF"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform
                        Angle="-30" />
                    <TranslateTransform
                        X="-6"
                        Y="-10" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
        <Rectangle
            Fill="#FCFCFC"
            Width="3"
            Height="8"
            RenderTransformOrigin="0.5,0.5">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform />
                    <TranslateTransform
                        Y="-12" />
                </TransformGroup>
            </UIElement.RenderTransform>
        </Rectangle>
    </Grid>
</UserControl>